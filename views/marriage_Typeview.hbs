     {{!-- <link rel='stylesheet' href='/stylesheets/button.css' /> --}}
    <div class="d-grid gap-2 d-md-flex justify-content-md-center pt-4">

{{#each eventtype}}
<a href="/select_marriage/{{this._id}}" class="btn btn-primary" tabindex="-1" role="button" aria-disabled="true">{{this.eventtype}}</a>
{{/each}}
</div>
 
 
 <section>
        <div class="container my-5">
          <div class="row">
            {{#each typeevent}}
              <div class="card col-md-3 m-2" style="width: 15rem;">
                <img style="width: 100%;height:200px" src="/images/{{this.image}}"  class="card-img-top">
                <div class="card-body">
                  <div class="form-check">
  {{!-- <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault" > --}}
 <a  class="form-check-input" type="checkbox"  value="" id="flexCheckDefault" href="/addcart_event/{{this._id}}"></a>

  <label class="form-check-label" for="flexCheckDefault">
{{this.eventname}}  </label>
</div>
                  {{!-- <h5 class="card-title">{{this.eventname}}</h5> --}}
                  <p class="card-text">Rs.{{this.amount}}</p>
                  {{!-- <a href="#" class="btn btn-primary">Add to Cart</a> --}}
                </div>
              </div>
            {{/each}}
          </div>
        </div>

      </section>