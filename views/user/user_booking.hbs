<div class="col py-3">

  {{!-- <section> --}}
    <div class="container my-5">
      <div class="row d-flex justify-content-center">
        {{#each eventresult}}
        <div class="card col-md-3 m-2" style="width: 15rem;">
          <img style="width: 100%;height:200px" src="/images/{{this.image}}" class="card-img-top pt-2">
          <div class="card-body">
            <div class="form-check">
              {{!-- <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault"> --}}
              {{!-- <a href="/select_event/{{this._id}}" class="form-check-input" type="button" value=""
                id="flexCheckDefault"></a> --}}
              {{!-- {{#each name}} --}}
              <label class="form-check-label text-center ps-2 fw-bold" for="flexCheckDefault">{{this.eventname}} </label>
              
              {{!-- {{/each}} --}}
            </div>
            {{!-- <h5 class="card-title">{{this.description}}</h5> --}}
            <p class="card-text text-center">{{this.description}}</p>
            <a href="/view_eventdetails/{{this._id}}" class="btn btn-primary text-center ms-1">Event Details</a>
          </div>
        </div>
        {{/each}}
      </div>
    </div>

    {{!--
  </section> --}}
</div>